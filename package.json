  "license": "cc-by-sa-4.0",
  "bugs": {
    "url": "https://github.com/ilkka/ilkka.io/issues"
  },
  "homepage": "https://github.com/ilkka/ilkka.io",
  "devDependencies": {
    "autoprefixer": "^5.1.0",
    "babel-core": "^5.0.12",
    "babel-loader": "^5.0.0",
    "concurrently": "0.0.5",
    "webpack": "^1.8.4"
  }
}
