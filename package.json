{
  "name": "ilkka.io",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrent -k 'npm run js-watch' 'compass watch'",
    "js": "webpack --progress --color",
    "js-watch": "webpack --watch --progress --color",
    "css": "compass compile",
    "prefixes": "autoprefixer -d themes/ilkka.io/static/css -b '> 5%' themes/ilkka.io/static/css/*.css",
    "publish": "npm run js && npm run css && npm run prefixes && git --work-tree public --git-dir public/.git add --all && git --work-tree public --git-dir public/.git commit -m 'Regenerated' && git --work-tree public --git-dir public/.git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ilkka/ilkka.io"
  },
